    <Compile Remove="Extensions\MonoHttp\HtmlEncoder.cs" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Extensions\MonoHttp\HttpUtility.cs" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Web" Condition="'$(TargetFramework)' == 'net452'" />
  </ItemGroup>
